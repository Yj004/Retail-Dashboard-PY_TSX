{
  "version": 2,
  "buildCommand": "",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "JWT_SECRET_KEY": "your-secure-jwt-secret-change-in-production",
    "NODE_ENV": "production"
  }
} 